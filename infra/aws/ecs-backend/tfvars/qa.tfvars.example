# Example values for qa
aws_region     = "us-east-1"
aws_account_id = "123456789012"
cluster_name   = "annual-sports-qa"
vpc_cidr       = "10.20.0.0/16"
name_prefix    = "as-qa"
service_discovery_namespace = "annual-sports.qa.local"

availability_zones = ["us-east-1a", "us-east-1b"]
public_subnets      = ["10.20.1.0/24", "10.20.2.0/24"]
private_subnets     = ["10.20.11.0/24", "10.20.12.0/24"]

api_domain = "sports-qa-api.your-domain.com"
route53_zone_id = "Z1234567890"
acm_certificate_arn = "arn:aws:acm:us-east-1:123456789012:certificate/replace-with-your-cert-id"

image_tag = "latest"

mongo_uri_secret_name = "as-qa-mongo-uri"

database_names = {
  "identity-service"             = "as-qa-identity"
  "enrollment-service"           = "as-qa-enrollment"
  "department-service"           = "as-qa-department"
  "sports-participation-service" = "as-qa-sports-part"
  "event-configuration-service"  = "as-qa-event-config"
  "scheduling-service"           = "as-qa-scheduling"
  "scoring-service"              = "as-qa-scoring"
  "reporting-service"            = "as-qa-reporting"
}

jwt_secret_name = "as-qa-jwt"
email_provider = "gmail"
gmail_user = "your-email@gmail.com"
gmail_app_password_secret_name = "as-qa-gmail-app-password"
sendgrid_api_key_secret_name = "as-qa-sendgrid-api-key"
resend_api_key_secret_name = "as-qa-resend-api-key"
smtp_password_secret_name = "as-qa-smtp-password"
email_from = "no-reply@your-domain.com"
