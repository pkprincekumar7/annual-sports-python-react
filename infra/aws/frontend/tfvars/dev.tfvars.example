# Core
aws_region  = "us-east-1"

# Buckets
bucket_name = "your-frontend-dev-bucket"
cloudfront_logs_bucket_name = "your-frontend-logs-bucket"

# Domain & certificate
domain     = "sports-dev.your-domain.com"
route53_zone_id = "Z1234567890"
cloudfront_acm_certificate_arn = "arn:aws:acm:us-east-1:123456789012:certificate/replace-with-your-cloudfront-cert-id"

# CloudFront
cloudfront_price_class = "PriceClass_100"
