# Example values for dev
aws_region     = "us-east-1"
aws_account_id = "123456789012"
cluster_name   = "annual-sports-dev"
vpc_cidr       = "10.10.0.0/16"
name_prefix    = "as-dev"
service_discovery_namespace = "annual-sports.dev.local"

availability_zones = ["us-east-1a", "us-east-1b"]
public_subnets      = ["10.10.1.0/24", "10.10.2.0/24"]
private_subnets     = ["10.10.11.0/24", "10.10.12.0/24"]

api_domain = "sports-dev-api.your-domain.com"
route53_zone_id = "Z1234567890"
acm_certificate_arn = "arn:aws:acm:us-east-1:123456789012:certificate/replace-with-your-cert-id"

image_tag = "latest"

mongo_uri_secret_name = "as-dev-mongo-uri"

database_names = {
  "identity-service"             = "as-dev-identity"
  "enrollment-service"           = "as-dev-enrollment"
  "department-service"           = "as-dev-department"
  "sports-participation-service" = "as-dev-sports-part"
  "event-configuration-service"  = "as-dev-event-config"
  "scheduling-service"           = "as-dev-scheduling"
  "scoring-service"              = "as-dev-scoring"
  "reporting-service"            = "as-dev-reporting"
}

jwt_secret_name = "as-dev-jwt"
email_provider = "gmail"
gmail_user = "your-email@gmail.com"
gmail_app_password_secret_name = "as-dev-gmail-app-password"
sendgrid_api_key_secret_name = "as-dev-sendgrid-api-key"
resend_api_key_secret_name = "as-dev-resend-api-key"
smtp_password_secret_name = "as-dev-smtp-password"
email_from = "no-reply@your-domain.com"
