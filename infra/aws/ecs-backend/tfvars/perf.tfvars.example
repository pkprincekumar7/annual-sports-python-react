# Example values for perf
aws_region     = "us-east-1"
aws_account_id = "123456789012"
cluster_name   = "annual-sports-perf"
vpc_cidr       = "10.40.0.0/16"
name_prefix    = "as-perf"
service_discovery_namespace = "annual-sports.perf.local"

availability_zones = ["us-east-1a", "us-east-1b"]
public_subnets      = ["10.40.1.0/24", "10.40.2.0/24"]
private_subnets     = ["10.40.11.0/24", "10.40.12.0/24"]

api_domain = "sports-perf-api.your-domain.com"
route53_zone_id = "Z1234567890"
acm_certificate_arn = "arn:aws:acm:us-east-1:123456789012:certificate/replace-with-your-cert-id"

image_tag = "latest"

mongo_uri_secret_name = "as-perf-mongo-uri"

database_names = {
  "identity-service"             = "as-perf-identity"
  "enrollment-service"           = "as-perf-enrollment"
  "department-service"           = "as-perf-department"
  "sports-participation-service" = "as-perf-sports-part"
  "event-configuration-service"  = "as-perf-event-config"
  "scheduling-service"           = "as-perf-scheduling"
  "scoring-service"              = "as-perf-scoring"
  "reporting-service"            = "as-perf-reporting"
}

jwt_secret_name = "as-perf-jwt"
email_provider = "gmail"
gmail_user = "your-email@gmail.com"
gmail_app_password_secret_name = "as-perf-gmail-app-password"
sendgrid_api_key_secret_name = "as-perf-sendgrid-api-key"
resend_api_key_secret_name = "as-perf-resend-api-key"
smtp_password_secret_name = "as-perf-smtp-password"
email_from = "no-reply@your-domain.com"
